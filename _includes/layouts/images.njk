{% for events in collections.events | reverse %}
    {% if events.data.documentation %}
        {# <ul class="event-container"> #}
            {% for documentation in events.data.documentation %}
            <li>
                <div class="event-inner">
                    <a href="{{ documentation }}" class="lightboxTrigger"><img src="{{ documentation }}" alt="{{ events.data.title }} {{ events.data.date | readableDate }} {{ events.data.time }} @ {{ events.data.venue }}"></a>
                </div>
            </li>
            {% endfor %}
        {# </ul> #}
    {% endif %}
{% endfor %}
